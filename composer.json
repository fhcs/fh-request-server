{
    "name": "fitnesshouse/fh-request-server",
    "description": "Fitness House Request Server Api through the message broker",
    "license": "MIT",
    "homepage": "https://github.com/fhcs/fh-request-server",
    "authors": [
        {
            "name": "Mikhaylov.V",
            "email": "mihailov.v@fhclub.ru"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-simplexml": "*",
        "illuminate/support": "6.*",
        "vladmeh/rabbitmq-client": "^1.2",
        "vladmeh/xml-utils": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "1.*"
    },
    "autoload": {
        "psr-4": {
            "Fh\\RequestServer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fh\\RequestServer\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fh\\RequestServer\\RequestServerServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
